{
    "name": "basecodeoy/measurements",
    "description": "A collection of commonly used measurements and units.",
    "license": "MIT",
    "keywords": [
        "BaseCode Oy",
        "laravel",
        "intl"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@basecode.sh",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/basecodeoy/measurements",
    "require": {
        "php": "^8.3",
        "ext-ctype": "*",
        "ext-mbstring": "*",
        "illuminate/contracts": "^11.0",
        "illuminate/support": "^11.0",
        "spatie/laravel-data": "^4.11.1"
    },
    "require-dev": {
        "basecodeoy/standards": "^1.1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "BaseCodeOy\\Measurements\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        },
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi"
    }
}
